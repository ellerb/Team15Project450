<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tcss450.uw.edu.team15project450">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AnotherActivity"
            android:label="@string/title_activity_another"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CreateTourActivity"
            android:label="Create a Tour" >
        </activity>
        <activity android:name=".ViewCreatedToursActivity"
            android:label="View Created Tours"/>
        <activity android:name=".BrowseToursActivity"
            android:label="Browse Tours" >
        </activity>
    </application>

</manifest>